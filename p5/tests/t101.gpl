int    lhs_int;
double lhs_double;
string lhs_string;
double ex;
double ex2;
int    rhs_int=22;
double rhs_double=33.33;
string rhs_string="Blobo";

on hkey
{
  print("Assignment. h-key pressed");
  lhs_int    = rhs_int;
  lhs_double = rhs_double;
  lhs_string = rhs_string;
}

on jkey
{
  print("Assignment. j-key pressed");
  lhs_double = rhs_double;
  lhs_string = rhs_int;
  ex = rhs_int;
}

on kkey
{
  print("Assignment. k-key pressed");
  lhs_string = rhs_double;
  ex2 = ex;
  ex += lhs_double;
}
